# devise.mailer.confirmation_instructions.greeting
msgid "Welcome %{recipient}!"
msgstr "%{recipient}님 환영합니다!"

# devise.registrations.signed_up
msgid "Welcome! You have signed up successfully."
msgstr "환영합니다! 성공적으로 가입되었습니다."

# devise.failure.already_authenticated
msgid "You are already signed in."
msgstr "이미 로그인되어 있습니다."

# devise.mailer.confirmation_instructions.instruction
msgid "You can confirm your account email through the link below:"
msgstr "아래의 링크를 클릭하시면 이메일 인증이 완료됩니다:"

# devise.passwords.no_token
msgid "You can't access this page without coming from a password reset email. If you do come from a password reset email, please make sure you used the full URL provided."
msgstr "비밀번호 재설정 이메일을 거치지 않고 이 페이지에 접근할 수 없습니다. 비밀번호 재설정 이메일에서 오셨다면, 주소가 제대로 되었는지 확인해 주세요."

# devise.failure.last_attempt
msgid "You have one more attempt before your account is locked."
msgstr "이번 로그인 시도를 실패하면 계정이 잠깁니다."

# devise.registrations.signed_up_but_locked
msgid "You have signed up successfully. However, we could not sign you in because your account is locked."
msgstr "성공적으로 가입되었지만, 계정이 잠겨있어 로그인 할 수 없습니다."

# devise.registrations.signed_up_but_inactive
msgid "You have signed up successfully. However, we could not sign you in because your account is not yet activated."
msgstr "성공적으로 가입되었지만, 아직 계정이 활성화되어 있지 않아 로그인 할 수 없습니다."

# devise.failure.unconfirmed
msgid "You have to confirm your email address before continuing."
msgstr "계속하기 전에 이메일 주소를 인증해야 합니다."

# devise.failure.unauthenticated
msgid "You need to sign in or sign up before continuing."
msgstr "계속하려면 로그인하거나 가입해야 합니다."

# devise.registrations.update_needs_confirmation
msgid "You updated your account successfully, but we need to verify your new email address. Please check your email and follow the confirm link to confirm your new email address."
msgstr "계정이 성공적으로 수정되었지만, 새로운 이메일 주소를 확인해야 합니다. 이메일로 전송된 이메일 인증 주소를 클릭하여 새로운 이메일 주소를 인증해 주세요."

# devise.confirmations.send_instructions
msgid "You will receive an email with instructions for how to confirm your email address in a few minutes."
msgstr "이메일로 이메일 주소 인증 안내가 발송되었습니다."

# devise.unlocks.send_instructions
msgid "You will receive an email with instructions for how to unlock your account in a few minutes."
msgstr "잠시 후 이메일로 계정 잠금 해제 안내를 받아보실 수 있습니다."

# devise.passwords.send_instructions
msgid "You will receive an email with instructions on how to reset your password in a few minutes."
msgstr "잠시 후에 비밀번호 재설정 안내 메일을 받을 수 있습니다."

# devise.mailer.unlock_instructions.message
msgid "Your account has been locked due to an excessive amount of unsuccessful sign in attempts."
msgstr "로그인 실패 횟수 초과로 귀하의 계정이 잠금 처리되었습니다."

# devise.unlocks.unlocked
msgid "Your account has been unlocked successfully. Please sign in to continue."
msgstr "계정이 성공적으로 잠금 해제되었습니다. 로그인하여 계속해 주세요."

# devise.registrations.updated
msgid "Your account has been updated successfully."
msgstr "계정이 성공적으로 수정되었습니다."

# devise.failure.locked
msgid "Your account is locked."
msgstr "계정이 잠겨있습니다."

# devise.failure.inactive
msgid "Your account is not activated yet."
msgstr "계정이 아직 활성화되지 않았습니다."

# devise.confirmations.confirmed
msgid "Your email address has been successfully confirmed."
msgstr "이메일 주소가 성공적으로 인증되었습니다."

# devise.passwords.updated_not_active
msgid "Your password has been changed successfully."
msgstr "비밀번호가 성공적으로 변경되었습니다."

# devise.passwords.updated
msgid "Your password has been changed successfully. You are now signed in."
msgstr "성공적으로 비밀번호를 변경했습니다. 로그인 되었습니다."

# devise.mailer.reset_password_instructions.instruction_3
msgid "Your password won't change until you access the link above and create a new one."
msgstr "위 링크에 접속하여 새로운 비밀번호를 생성하기 전까지 귀하의 비밀번호는 변경되지 않습니다."

# devise.failure.timeout
msgid "Your session expired. Please sign in again to continue."
msgstr "세션이 만료되었습니다. 계속하려면 다시 로그인해야 합니다."

# errors.messages.expired
msgid "has expired, please request a new one"
msgstr "이(가) 만료되었습니다 새로 요청해 주세요."

# devise.registrations.edit.leave_blank_if_you_don_t_want_to_change_it
msgid "leave blank if you don't want to change it"
msgstr "변경을 원하지 않으시면 빈 칸으로 남겨주세요"

# errors.messages.confirmation_period_expired
msgid "needs to be confirmed within %{period}, please request a new one"
msgstr "%{period} 이내에 이메일 인증을 해야 합니다. 새로 요청해 주세요."

# errors.messages.not_found
msgid "not found"
msgstr "찾을 수 없습니다."

# errors.messages.already_confirmed
msgid "was already confirmed, please try signing in"
msgstr "은(는) 이미 인증되었습니다. 다시 로그인해 보세요."

# errors.messages.not_locked
msgid "was not locked"
msgstr "은(는) 잠기지 않았습니다."

# devise.registrations.edit.we_need_your_current_password_to_confirm_your_changes
msgid "we need your current password to confirm your changes"
msgstr "변경 사항을 반영하려면 현재의 비밀번호를 입력하세요"
