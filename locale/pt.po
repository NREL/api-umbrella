msgid "Forgot your password?"
msgstr "Esqueceu a palavra-passe?"

# devise.mailer.unlock_instructions.greeting
msgid "Hello %{recipient}!"
msgstr "Olá %{recipient}!"

# devise.mailer.reset_password_instructions.instruction_2
msgid "If you didn't request this, please ignore this email."
msgstr "Se não fez este pedido, por favor ignore este e-mail."

# devise.unlocks.send_paranoid_instructions
msgid "If your account exists, you will receive an email with instructions for how to unlock it in a few minutes."
msgstr "Se a sua conta existir, irá receber dentro de alguns minutos um email com instruções para a desbloquear."

# devise.passwords.send_paranoid_instructions
msgid "If your email address exists in our database, you will receive a password recovery link at your email address in a few minutes."
msgstr "Se o seu e-mail existir na nossa base de dados, irá receber dentro de alguns minutos um e-mail com instruções para recuperar a sua senha."

# devise.confirmations.send_paranoid_instructions
msgid "If your email address exists in our database, you will receive an email with instructions for how to confirm your email address in a few minutes."
msgstr "Se o seu email existir na nossa base de dados, irá receber dentro de alguns minutos um email com instruções para confirmar a sua conta."

# devise.failure.not_found_in_database
msgid "Invalid %{authentication_keys} or password."
msgstr "O endereço de email ou a palavra-passe são inválidos."

# activerecord.attributes.user.remember_me
msgid "Remember me"
msgstr "Lembrar-se de mim"

# devise.confirmations.new.resend_confirmation_instructions
msgid "Resend confirmation instructions"
msgstr "Reenviar instruções de confirmação"

# devise.unlocks.new.resend_unlock_instructions
msgid "Resend unlock instructions"
msgstr "Renviar instruções de desbloqueio"

# devise.mailer.reset_password_instructions.subject
msgid "Reset password instructions"
msgstr "Instruções de recuperação de palavra-passe"

# devise.passwords.new.send_me_reset_password_instructions
msgid "Send me reset password instructions"
msgstr "Enviar-me instruções para repor a senha"

# devise.shared.links.sign_in
msgid "Sign in"
msgstr "Login"

# devise.shared.links.sign_in_with_provider
msgid "Sign in with %{provider}"
msgstr "Entrar com %{provider}"

# devise.shared.links.sign_up
msgid "Sign up"
msgstr "Criar conta"

# devise.sessions.signed_in
msgid "Signed in successfully."
msgstr "Autenticação efectuada com sucesso."

# devise.sessions.signed_out
msgid "Signed out successfully."
msgstr "Saiu da sessão com sucesso."

# devise.mailer.reset_password_instructions.instruction
msgid "Someone has requested a link to change your password, and you can do this through the link below."
msgstr "Foi efectuado um pedido para repor a sua palavra-passe. Para o fazer click no link abaixo."

# devise.omniauth_callbacks.success
msgid "Successfully authenticated from %{kind} account."
msgstr "A sua conta foi autenticada por %{kind} com sucesso."

# devise.mailer.unlock_instructions.subject
msgid "Unlock instructions"
msgstr "Instruções de desbloqueio"

# devise.mailer.unlock_instructions.action
msgid "Unlock my account"
msgstr "Desbloquear a minha conta"

# devise.mailer.confirmation_instructions.greeting
msgid "Welcome %{recipient}!"
msgstr "Bem-vindo %{recipient}!"

# devise.registrations.signed_up
msgid "Welcome! You have signed up successfully."
msgstr "Bemvindo! A autenticação foi efectuada com sucesso."

# devise.failure.already_authenticated
msgid "You are already signed in."
msgstr "Já se encontra autenticado."

# devise.mailer.confirmation_instructions.instruction
msgid "You can confirm your account email through the link below:"
msgstr "Pode confirmar a sua conta através do link:"

# devise.passwords.no_token
msgid "You can't access this page without coming from a password reset email. If you do come from a password reset email, please make sure you used the full URL provided."
msgstr "Não pode aceder a esta página sem vir do email de redefinição da senha. Se vem dum email para redefinir a senha, por favor certifique-se que usou o URL completo que foi enviado."

# devise.failure.last_attempt
msgid "You have one more attempt before your account is locked."
msgstr "Tem mais uma tentativa antes da sua conta ser bloqueada."

# devise.registrations.signed_up_but_locked
msgid "You have signed up successfully. However, we could not sign you in because your account is locked."
msgstr "Registou-se com sucesso. No entanto, não podemos iniciar a sua sessão porque a sua conta encontra-se bloqueada."

# devise.registrations.signed_up_but_inactive
msgid "You have signed up successfully. However, we could not sign you in because your account is not yet activated."
msgstr "Registou-se com sucesso. No entanto, não podemos iniciar a sua sessão porque ainda não confirmou a sua conta."

# devise.failure.unconfirmed
msgid "You have to confirm your email address before continuing."
msgstr "Tem de confirmar a sua conta antes de continuar."

# devise.failure.unauthenticated
msgid "You need to sign in or sign up before continuing."
msgstr "Antes de continuar tem de se autenticar ou efectuar um registo."

# devise.registrations.update_needs_confirmation
msgid "You updated your account successfully, but we need to verify your new email address. Please check your email and follow the confirm link to confirm your new email address."
msgstr "A sua conta foi actualizada com sucesso, mas precisamos de confirmar o seu email. Por favor veja o seu email e clique no link para finalizar a confirmação do seu email."

# devise.confirmations.send_instructions
msgid "You will receive an email with instructions for how to confirm your email address in a few minutes."
msgstr "Dentro de alguns minutos irá receber um email com instruções para confirmar a sua conta."

# devise.unlocks.send_instructions
msgid "You will receive an email with instructions for how to unlock your account in a few minutes."
msgstr "Dentro de alguns minutos irá receber um email com as instruções para desbloquear a sua conta."

# devise.passwords.send_instructions
msgid "You will receive an email with instructions on how to reset your password in a few minutes."
msgstr "Dentro de alguns minutos irá receber um email com instruções de reinicialização da palavra-passe."

# devise.mailer.unlock_instructions.message
msgid "Your account has been locked due to an excessive amount of unsuccessful sign in attempts."
msgstr "A sua conta foi bloqueada devido a um número excessivo de tentativas de acesso inválidas."

# devise.unlocks.unlocked
msgid "Your account has been unlocked successfully. Please sign in to continue."
msgstr "A sua conta foi desbloqueada com sucesso. Por favor autentique-se para continuar."

# devise.registrations.updated
msgid "Your account has been updated successfully."
msgstr "A sua conta foi actualizada com sucesso."

# devise.failure.locked
msgid "Your account is locked."
msgstr "A sua conta está bloqueada."

# devise.failure.inactive
msgid "Your account is not activated yet."
msgstr "A sua conta ainda não foi activada."

# devise.confirmations.confirmed
msgid "Your email address has been successfully confirmed."
msgstr "A sua conta foi confirmada com sucesso."

# devise.passwords.updated_not_active
msgid "Your password has been changed successfully."
msgstr "A sua password foi alterada com sucesso."

# devise.passwords.updated
msgid "Your password has been changed successfully. You are now signed in."
msgstr "A sua palavra-passe foi alterada com sucesso. Agora está autenticado."

# devise.mailer.reset_password_instructions.instruction_3
msgid "Your password won't change until you access the link above and create a new one."
msgstr "A sua palavra-passe só será alterada quando aceder ao link abaixo para a alterar."

# devise.failure.timeout
msgid "Your session expired. Please sign in again to continue."
msgstr "A sua sessão expirou, por favor autentique-se novamente para continuar."

# errors.messages.expired
msgid "has expired, please request a new one"
msgstr "expirou, por favor solicite um novo"

# devise.registrations.edit.leave_blank_if_you_don_t_want_to_change_it
msgid "leave blank if you don't want to change it"
msgstr "deixe em branco caso não queira alterar"

# errors.messages.confirmation_period_expired
msgid "needs to be confirmed within %{period}, please request a new one"
msgstr "necessitava de confirmação até %{period}, por favor faça um novo pedido"

# errors.messages.not_found
msgid "not found"
msgstr "não foi encontrado"

# errors.messages.already_confirmed
msgid "was already confirmed, please try signing in"
msgstr "já foi confirmado, por favor tente efectuar a autenticação"

# errors.messages.not_locked
msgid "was not locked"
msgstr "não foi bloqueado"

# devise.registrations.edit.we_need_your_current_password_to_confirm_your_changes
msgid "we need your current password to confirm your changes"
msgstr "precisamos da sua senha actual para confirmar as alterações"
