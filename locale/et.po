msgstr "Oma konto saad kinnitada alloleva lingi kaudu:"

# devise.passwords.no_token
msgid "You can't access this page without coming from a password reset email. If you do come from a password reset email, please make sure you used the full URL provided."
msgstr "Sa ei saa seda lehte külastada mujalt kui salasõna taastamise e-kirjast. Kui sa tuled salasõna taastamise e-kirjast, palun kontrolli, et sa kasutasid täielikku URLi."

# devise.failure.last_attempt
msgid "You have one more attempt before your account is locked."
msgstr "Sinul on jäänud veel üks katse enne kui su konto lukustatakse."

# devise.registrations.signed_up_but_locked
msgid "You have signed up successfully. However, we could not sign you in because your account is locked."
msgstr "Sa registreerusid edukalt. Siiski ei saa me sind sisse logida, sest su konto on lukus."

# devise.registrations.signed_up_but_inactive
msgid "You have signed up successfully. However, we could not sign you in because your account is not yet activated."
msgstr "Sa registreerusid edukalt. Siiski ei saa me sind sisse logida, sest su konto pole veel aktiveeritud."

# devise.failure.unconfirmed
msgid "You have to confirm your email address before continuing."
msgstr "Sa pead oma konto kinnitama, enne jätkamist."

# devise.failure.unauthenticated
msgid "You need to sign in or sign up before continuing."
msgstr "Sa pead sisse logima või registreeruma enne jätkamist."

# devise.registrations.update_needs_confirmation
msgid "You updated your account successfully, but we need to verify your new email address. Please check your email and follow the confirm link to confirm your new email address."
msgstr "Sa uuendasid edukalt oma kontot, kuid me peame kontrollima sinu e-posti aadressi. Palun kontrolli oma e-posti ja ava kinnitamise link, et viia lõpule aadressi kinnitamine."

# devise.confirmations.send_instructions
msgid "You will receive an email with instructions for how to confirm your email address in a few minutes."
msgstr "Mõne minuti jooksul saad sa e-kirja juhistega oma konto kinnitamiseks."

# devise.unlocks.send_instructions
msgid "You will receive an email with instructions for how to unlock your account in a few minutes."
msgstr "Sa saad mõne minuti jooksul e-kirja juhistega, kuidas oma konto lukust vabastada."

# devise.passwords.send_instructions
msgid "You will receive an email with instructions on how to reset your password in a few minutes."
msgstr "Mõne minuti jooksul saad sa kirja juhistega, kuidas oma salasõna taastada."

# devise.mailer.unlock_instructions.message
msgid "Your account has been locked due to an excessive amount of unsuccessful sign in attempts."
msgstr "Sinu konto on lukustatud liigsete ebaõnnestunud sisselogimiskatsete tõttu"

# devise.unlocks.unlocked
msgid "Your account has been unlocked successfully. Please sign in to continue."
msgstr "Sinu konto on edukalt lukust vabastatud. Jätkamiseks palun logi sisse."

# devise.registrations.updated
msgid "Your account has been updated successfully."
msgstr "Sa uuendasid edukalt oma kontot."

# devise.failure.locked
