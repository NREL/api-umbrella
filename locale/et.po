msgstr "Sinu konto lukustati."

# devise.failure.inactive
msgid "Your account is not activated yet."
msgstr "Sinu konto polnud veel aktiveeritud."

# devise.confirmations.confirmed
msgid "Your email address has been successfully confirmed."
msgstr "Sinu konto on edukalt kinnitatud."

# devise.passwords.updated_not_active
msgid "Your password has been changed successfully."
msgstr "Sinu salasõna vahetati edukalt."

# devise.passwords.updated
