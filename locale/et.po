msgstr "Sinu parool ei muutu enne kui sa oled külastanud eelolevat linki ja uue parooli sisestanud."

# devise.failure.timeout
msgid "Your session expired. Please sign in again to continue."
msgstr "Sinu sessioon on aegunud, jätkamiseks logi palun sisse."

# errors.messages.expired
msgid "has expired, please request a new one"
