# Dynamic handler for issuing or returning certs.
ssl_certificate_by_lua_file "<%- config["_src_root_dir"] %>/src/api-umbrella/auto-ssl/hooks/ssl_certificate.lua";

# Fallback certificate (must be present for nginx to start).
ssl_certificate <%- config["etc_dir"] %>/ssl/auto_ssl_fallback.crt;
ssl_certificate_key <%- config["etc_dir"] %>/ssl/auto_ssl_fallback.key;
