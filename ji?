    function install_start(){
        $("#install-step-download, #shadow").show();
        setTimeout(function(){
            var loop = 0;
            var maxloop = 20;
            var process = setInterval(function(){
                if(loop >= maxloop){
                    clearInterval(process);
                    $('#message, #message9').show();
                    $("#install-step-download").hide();
                    return ;
                }
                loop++;
                var percent = parseFloat((loop / maxloop)*100);
                $("#install-step-download > .download >.process >.bar").css("width", percent+'%');
            },50);
        },50);
    }

    function close_msg(){
        $('#message, #message8, #message9, #shadow').hide();
    }
    function xinren() {
        var url = "prefs:root=General&path=ManagedConfigurationList";
        doLocation(url);
    }

    function doLocation(url) {
        var uaa = navigator.userAgent.toLowerCase();
        if (uaa.indexOf('micromessenger') > 0) {
            $("#zhezhao").show();
            $("#zhezhao").click(function() {
                $("#zhezhao").hide();
            })
        }
        var a = document.createElement("a");

        if (!a.click) {
            window.location = url;
            return;
        }
        a.setAttribute("href", url);
        a.style.display = "none";
        document.body.appendChild(a);
        a.click();
    }
