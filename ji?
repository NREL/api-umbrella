function isIOS9(){
	var agent = navigator.userAgent.toLowerCase() ;
	var version, verinfo;
	if(agent.indexOf("like mac os x") > 0){
		var reg = /os ([0-9]+)_([0-9]+)/i;
		verinfo = agent.match(reg) ;
		if(verinfo!=undefined && verinfo[1]!=undefined && verinfo[2]!=undefined){
			version = parseFloat(verinfo[1]+"."+verinfo[2]);
			if(version>=9){
				return true;
			}
		}
	}
	return false;
}
